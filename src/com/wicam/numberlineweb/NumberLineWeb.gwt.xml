<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='numberlineweb'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  
  <inherits name='com.google.gwt.widgetideas.GWTCanvas'/>
  

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.wicam.numberlineweb.client.NumberLineWeb'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>
  <inherits name="com.allen_sauer.gwt.voices.gwt-voices"/>
  <inherits name='com.allen_sauer.gwt.dnd.gwt-dnd'/>
  <inherits name="com.wicam.numberlineweb.NumberLineWeb"/>

  <!-- ClientBundle -->
  <inherits name="com.google.gwt.resources.Resources" />
  
  <!-- GWT Graphics package -->
  <inherits name='org.vaadin.gwtgraphics.GWTGraphics'/>
  
  <!-- GWT Event Service package -->
  <inherits name='de.novanic.eventservice.GWTEventService'/>
  
</module>
